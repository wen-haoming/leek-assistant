{
  "manifest_version": 3,
  "name": "My Vue Chrome Ext",
  "version": "0.0.1",
  "description": "Chrome 插件",
  "icons": {
    "16": "icons/icon.png",
    "19": "icons/icon.png",
    "38": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
},
  "action": {
    "default_title": "Vue Chrome Ext",
    "default_icon": "icons/icon.png"
  },
  "background": {
    "service_worker": "background/service-worker.js"
  },
  "permissions": [
    "proxy",
    "tabs",
    "webRequest",
    "proxy",
    "sidePanel"
  ],
  "optional_permissions": [
    "https://searchapi.eastmoney.com"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "host_permissions": [
    
  ],
  "side_panel": {
    "default_path": "sidePanel/index.html"
  },
  "content_scripts": [
    {
      "js": [
        "content/content.js"
      ],
      "matches": [
        "http://127.0.0.1:5500/*"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "match_about_blank": true
    }
  ]
}
